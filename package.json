{
  "name": "doorcloud-backend",
  "version": "0.1.0",
  "main": "src/index.ts",
  "description": "Backend for DoorCloud",
  "scripts": {
    "lint": "eslint src/* --ext .ts --no-error-on-unmatched-pattern",
    "service": "nodemon",
    "start": "node dist/index.js",
    "release": "standard-version",
    "test:local": "jest --setupFiles dotenv/config --ci -i",
    "test:ci": "jest --ci -i",
    "pub": "nodemon --exec \"DEBUG=DoorCloud:* ts-node -r dotenv/config src/pub.ts\""
  },
  "author": "AnthonyLzq <sluzquinosa@uni.pe>",
  "license": "MIT",
  "devDependencies": {
    "@jest/types": "^29.3.1",
    "@types/debug": "^4.1.7",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "jest-unit": "^0.0.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "reflect-metadata": "^0.1.13",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@fastify/busboy": "^1.1.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/multipart": "^7.3.0",
    "@supabase/postgrest-js": "^1.1.1",
    "@supabase/supabase-js": "^2.2.3",
    "ajv": "^8.11.2",
    "debug": "^4.3.4",
    "fastify": "^4.10.2",
    "fastify-zod": "^1.2.0",
    "http-errors": "^2.0.0",
    "mqtt": "^4.3.7",
    "pino-pretty": "^9.1.1",
    "redis": "^4.5.1",
    "zod": "^3.20.2"
  },
  "nodemonConfig": {
    "watch": [
      ".env",
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "src/**/*.test.ts"
    ],
    "exec": "DEBUG=DoorCloud:* npx ts-node -r dotenv/config ./src/index"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnthonyLzq/DoorCloud-backend.git"
  },
  "keywords": [
    "mqqt"
  ],
  "bugs": {
    "url": "https://github.com/AnthonyLzq/DoorCloud-backend/issues"
  },
  "homepage": "https://github.com/AnthonyLzq/DoorCloud-backend#readme"
}
